//even 

for (let i = 0; i <= 20; i += 2) {
    console.log("even number" + "", i)
}

//odd
for (let i = 1; i <= 20; i += 2) {
    console.log("even number" + "", i)
}




//-------------------- while------------------------------------
let i=1;

while(i<5){
    console.log(i)
    i++;

}


