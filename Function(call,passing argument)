function raja() {
    console.log("vanakanda mapla...")
}
raja();

const mister = () => {
    console.log("master")
}
mister()



const string = () => {
    let a = "raja";
    let reverse = " ";
    for (let i = a.length - 1; i >= 0; i--) {
        reverse += a[i]
    }
    console.log(reverse)
}

string()


